package org.example;

public class Untitled {
    public static void main(String[] args) {
       int cost = 2000000;
        System.out.println("cost = " + cost);
        double percent = 35.0;
        System.out.println("percent = " + percent);
        double pv = cost/100*percent;
        System.out.println("pv = " + pv);
    }
}
